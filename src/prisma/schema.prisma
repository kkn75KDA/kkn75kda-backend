generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String   @db.VarChar(30)
  email     String   @unique
  password  String   @db.VarChar(20)
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
